@model IEnumerable<InsuranceManagement.Models.CommercialAuto.CommercialAutoList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

@if (TempData.ContainsKey("SaveResult"))
{
    <div class="alert alert-success fade in" role="alert">
        @ViewBag.SaveResult
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
        @TempData["SaveResult"]
    </div>
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AutoID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientID)
        <th>
            @Html.DisplayNameFor(model => model.NumberInFleet)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.AutoID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ClientID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NumberInFleet)
    </td>

    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.AutoID }) |
        @Html.ActionLink("Details", "Details", new { id = item.AutoID }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.AutoID })
    </td>
</tr>
    }

</table>
